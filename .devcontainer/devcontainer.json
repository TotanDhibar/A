{
  "name": "Ubuntu 20.04 Codespace",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "remoteUser": "vscode",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "ms-vscode.cpptools",
    "ms-vscode.csharp",
    "ms-python.python"
  ],
  "forwardPorts": [],
  "mounts": [
    "source=${localEnv:SSH_AUTH_SOCK},target=/ssh-agent.sock,type=bind"
  ],
  "remoteEnv": {
    "SSH_AUTH_SOCK": "/ssh-agent.sock"
  },
  "postCreateCommand": "echo \"Welcome to your Ubuntu 20.04 Codespace!\""
}