{
  "name": "Ubuntu 20.04 Codespace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "remoteUser": "devuser",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "mounts": [
    "source=/workspaces,target=/workspaces,type=bind,consistency=cached"
  ]
}
